{
  "configurations": [
    {
      "type": "java",
      "name": "Spring Boot-SwtpApplication<swtp>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "de.hsrm.mi.team3.swtp.SwtpApplication",
      "projectName": "swtp",
      "args": "",
      "envFile": "${workspaceFolder}/.env"
    }
  ],
  "settings": {
    "vetur.validation.template": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "[vue]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
  }
}
